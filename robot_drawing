  triangle(160,40,245,40,200,60);


  fill(255,255,100);
  rect(50, 90, 100, 35, 20, 20);
  rect(250, 90, 100, 35, 20, 20);


  fill(0,0,0); 
  triangle(155, 85, 155, 105, 180, 85);
  triangle(245, 85, 245, 110, 215, 85);


  fill(0,0,200);  
  triangle(200,200,150,250,250,250);


  fill(255,0,0);
  quad(199, 80, 145, 120, 200, 130, 250, 120);
  quad(195, 195, 145, 245, 160, 170, 200, 140);
  quad(205, 140, 240, 170, 255, 245, 205, 195);


  fill(100);
  rect(80, 130, 35, 70);
  rect(290, 130, 35, 70);


  fill(255,0,0);
  quad(210, 135, 255, 125, 310, 140, 245, 165);
  quad(195, 135, 140, 125, 90, 140, 155, 165);


  fill(0,0,0);
  ellipse(98, 215, 40, 35);
  ellipse(308, 215, 40, 35);


