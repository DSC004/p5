    triangle(155, 275, 100, 320, 120, 210);
  fill(191, 165, 63); 
    triangle(120, 210, 155, 275, 235, 175);
  fill(255, 204, 0)
    triangle(120, 210, 100, 320, 45, 275);
  fill(242, 196, 12);
    triangle(235, 250, 235, 120, 155, 120);
  fill(229, 118, 25);
    triangle(235, 250, 235, 120, 315, 120);

  
  
  fill(216, 181, 38);
    triangle(235, 100, 155, 120, 195, 25);
  fill(204, 173, 51);
    triangle(235, 100, 315, 120, 275, 25);
  fill(191, 165, 63);   
    triangle(215, 80, 175, 100, 195, 45);
  fill(255, 204, 0);
    triangle(255, 80, 295, 100, 275, 45);
  fill(242, 196, 12);
    triangle(235, 100, 170, 150, 155, 120);
  fill(229, 118, 25);
    triangle(235, 100, 300, 150, 315, 120);
  fill(216, 181, 38);
    triangle(235, 100, 210, 175, 170,150);
  fill(204, 173, 51);
    triangle(235, 100, 260, 175, 300, 150);
  fill(255, 204, 0);
    triangle(210, 175, 260, 175, 235, 100);
  fill(242, 196, 12);
    triangle(235, 175, 215, 150, 195, 175);
  fill(229, 118, 25);
    triangle(235, 175, 255, 150, 275, 175);
  fill(216, 181, 38);
    triangle(215, 175, 255, 175, 235, 195);
  

  fill(255, 51, 39);   
    triangle(235, 100, 195, 85, 275, 85);

  
  fill(255, 204, 0);
  fill(242, 196, 12);
  fill(229, 118, 25);
  fill(216, 181, 38);
  fill(204, 173, 51);
