function setup() {
  createCanvas(400, 400);
}

function draw() {
  background(0);
  
 strokeWeight(2); 
  
  fill(255, 204, 0)
    triangle(235, 250, 155, 275, 235, 150);
  fill(242, 196, 12);
    triangle(235, 250, 155, 275, 300, 285);
  fill(229, 118, 25);
    triangle(155, 275, 300, 285, 100, 320);
